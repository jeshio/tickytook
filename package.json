{
  "name": "tickytook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild:storybook": "yarn test:generate-output",
    "storybook": "yarn prebuild:storybook && yarn start-storybook -p 9001 -c .storybook",
    "test:generate-output": "yarn jest --json --outputFile=jest-test-results.json || true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "export CI=true && yarn build && yarn lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-redux": "^7.0.6",
    "@types/react-textarea-autosize": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.1.14",
    "@types/styled-system": "^4.1.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "polished": "^3.2.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.8",
    "react-textarea-autosize": "^7.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "rsuite": "^3.8.3",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "typescript": "3.4.1",
    "utility-types": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-jest": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/redux-logger": "^3.0.7",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "redux-logger": "^3.0.6",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
