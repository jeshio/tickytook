{
  "name": "tickytook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "cross-env NODE_ENV=production node ./build/server/server/index.js",
    "start:dev": "nodemon ./build/server/server/index.js",
    "build": "react-scripts build && yarn build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild:storybook": "yarn test:generate-output",
    "storybook": "yarn prebuild:storybook && yarn start-storybook -p 9001 -c .storybook",
    "test:generate-output": "yarn jest --json --outputFile=jest-test-results.json || true",
    "benchmark": "autocannon http://localhost:3000 -c100",
    "benckmark:docker": "clinic docker -- npm run start:prod",
    "benchmark:bubbleprof": "clinic bubbleprof -- npm run start:prod",
    "benchmark:flame": "NODE_ENV=production clinic flame -- node ./dist/server/server/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "export CI=true && yarn build && yarn lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/immutability-helper": "^2.6.3",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-helmet": "^5.0.9",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-textarea-autosize": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.1.14",
    "@types/styled-system": "^4.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "immutability-helper": "^3.0.1",
    "lodash": "^4.17.11",
    "polished": "^3.2.0",
    "raw.macro": "^0.3.0",
    "react": "^16.8.6",
    "react-adsense": "^0.1.0",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.1.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.8",
    "react-share": "^3.0.0",
    "react-textarea-autosize": "^7.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "rsuite": "^3.8.3",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "typescript": "3.4.1",
    "utility-types": "^3.5.0"
